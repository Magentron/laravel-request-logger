{
  "name": "magentron/laravel-request-logger",
  "description": "HTTP request logger middleware for Laravel",
  "keywords": ["laravel", "logger", "request", "response"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Andrade",
      "homepage": "http://andersonandra.de",
      "email": "contato@andersonandra.de",
      "role": "Developer"
    },
    {
      "name": "Jeroen Derks",
      "email": "jeroen@derks.it",
      "homepage": "https://www.derks.it/",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/Magentron/laravel-request-logger",
  "support": {
    "email": "info@magentron.com",
    "issues":"https://github.com/magentron/laravel-request-logger/issues",
    "wiki":"https://github.com/magentron/laravel-request-logger",
    "source":"https://github.com/magentron/laravel-request-logger",
    "docs": "http://andersao.github.io/laravel-request-logger"
  },
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": ">=5.6.0",
    "illuminate/http": ">=5.6.0",
    "monolog/monolog": ">=1.12.0"
  },
  "require-dev" : {
    "laravel/framework": ">=5.6.0",
    "vimeo/psalm": "^4.22",
    "phpstan/phpstan": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.3"
  },
  "replace": {
	  "prettus/laravel-request-logger": ">1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\RequestLogger\\": "src/Prettus/RequestLogger/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prettus\\RequestLogger\\Providers\\LoggerServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-develop": "1.2-dev"
    }
  },
  "minimum-stability": "stable"
}
